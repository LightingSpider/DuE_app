<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#120031"
    tools:context=".EditProfileNew">

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:text="@string/edit_your_profile"
        android:textColor="#FFFFFF"
        android:textSize="26sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextEmailAddress55"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="34dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/jack"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView55" />

    <Spinner
        android:id="@+id/spinner_genderr"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/gender1"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/gender1"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress55" />

    <EditText
        android:id="@+id/editTextNumberr"
        android:layout_width="90dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:ems="10"
        android:hint="@string/_36"
        android:inputType="number"
        android:paddingStart="6dp"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingEnd="6dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHint="#E6FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_genderr" />

    <Spinner
        android:id="@+id/spinner_interestedinnn"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/interestedin"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/interestedin"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberr" />

    <EditText
        android:id="@+id/editTextTextEmailAddress77"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/instagram_m_jack"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_interestedinnn" />

    <EditText
        android:id="@+id/editTextTextEmailAddress66"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/facebook_jack_mil"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress77" />

    <Button
        android:id="@+id/btnsave"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btnround"
        android:text="@string/save"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        android:textStyle="bold"
        app:backgroundTint="#AB6CBB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress66" />

    <ImageView
        android:id="@+id/imageView2dd"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="@drawable/borderfortopnavbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/duemylogo1" />

</androidx.constraintlayout.widget.ConstraintLayout>