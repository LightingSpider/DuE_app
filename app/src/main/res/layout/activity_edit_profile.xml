<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#120031"
    tools:context=".EditProfile">

    <Button
        android:id="@+id/btnsave"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btnround"
        android:text="SAVE"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        android:textStyle="bold"
        app:backgroundTint="#AB6CBB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress6" />

    <EditText
        android:id="@+id/editTextTextEmailAddress7"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="Instagram: m_jack"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_interestedinnn" />

    <Spinner
        android:id="@+id/spinner_interestedinnn"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/interestedin"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/interestedin"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber" />

    <EditText
        android:id="@+id/editTextTextEmailAddress6"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="Facebook: Jack Mil"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress7" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="34dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="Jack"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#E6FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:text="Edit Your Profile"
        android:textColor="#FFFFFF"
        android:textSize="26sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_genderr"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/gender1"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/gender1"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress5" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="90dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:ems="10"
        android:hint="36"
        android:inputType="number"
        android:paddingStart="6dp"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingEnd="6dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHint="#E6FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_genderr" />

    <ImageView
        android:id="@+id/imageView2dd"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="@drawable/borderfortopnavbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/duemylogo1" />

</androidx.constraintlayout.widget.ConstraintLayout>