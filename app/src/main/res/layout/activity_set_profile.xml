<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#120031"
    tools:context=".SetProfile">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#9B3B7B"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Create Account"
        app:titleTextColor="#FFFFFF" />

    <Button
        android:id="@+id/btnnext"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btnround"
        android:text="@string/next"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        android:textStyle="bold"
        app:backgroundTint="#AB6CBB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress6" />

    <EditText
        android:id="@+id/editTextTextEmailAddress7"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/instaacc"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#B7FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_interestedinn" />

    <!--<TextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:background="#25A9A5FF"
        android:completionHint="@string/interestedin"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="@string/interestedin"
        android:textColor="#B7FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber" />-->

    <Spinner
        android:id="@+id/spinner_interestedinn"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/interestedin"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/interestedin"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber" />

    <EditText
        android:id="@+id/editTextTextEmailAddress6"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/fbacc"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#B7FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress7" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="34dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:drawablePadding="8dp"
        android:ems="15"
        android:hint="@string/name"
        android:includeFontPadding="false"
        android:inputType="textEmailAddress"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColor="@color/white"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#B7FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textCursorDrawable="@drawable/ic_launcher_mylogo_background"
        android:textIsSelectable="false"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/setupyourprofile"
        android:textColor="#FFFFFF"
        android:textSize="26sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

    <Spinner
        android:id="@+id/spinner_gender"
        android:layout_width="360dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:animateLayoutChanges="true"
        android:background="#25A9A5FF"
        android:completionHint="@string/gender1"
        android:drawablePadding="8dp"
        android:dropDownSelector="@color/pink"
        android:foregroundTint="#AB6CBB"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:popupBackground="@color/pink"
        android:popupElevation="20dp"
        android:text="@string/gender1"
        android:textColor="#AB6CBB"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress5" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="90dp"
        android:layout_height="36dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:background="#25A9A5FF"
        android:ems="10"
        android:hint="@string/age"
        android:inputType="number"
        android:paddingStart="6dp"
        android:paddingLeft="6dp"
        android:paddingTop="3dp"
        android:paddingEnd="6dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:textColorHint="#B7FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_gender" />

</androidx.constraintlayout.widget.ConstraintLayout>